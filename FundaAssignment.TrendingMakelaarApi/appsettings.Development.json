{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Filters": {
    "FilterSearchTerms": [ "/amsterdam", "/amsterdam/tuin" ]
  },
  "TrendingMakelaarCalculation": {
    "RefreshInterval": "12:00:00"
  },
  "BackgroundProcessing": {
    "RefreshCalculatedMakelaarData": {
      "Interval": "00:00:01",
      "PerformInitializationRun": true,
      "Retry": {
        "MaxAttempts": 3,
        "InitialDelay": "00:00:30",
        "BackoffFactor": 2.0,
        "MaxDelay": "00:05:00"
      }
    }
  },
  "FundaApi": {
    "BaseUrl": "https://partnerapi.funda.nl/feeds/Aanbod.svc/json/",
    "ApiKey": "TO_BE_FILLED_VIA_ENV_VARIABLE",
    "Retry": {
      "RateLimitDelay": "00:01:00",
      "MaxRetries": 3
    }
  }
}
